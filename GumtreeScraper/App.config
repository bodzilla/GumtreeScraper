    <add key="ScrapeCorsa" value="8 https://www.gumtree.com/search?search_category=cars&amp;search_location=e10lj&amp;vehicle_make=vauxhall&amp;vehicle_model=corsa&amp;distance=50&amp;max_price=2000&amp;min_price=500&amp;photos_filter=true&amp;vehicle_mileage=up_to_80000" />
    <add key="ScrapeFiesta" value="5 https://www.gumtree.com/search?search_category=cars&amp;search_location=e10lj&amp;vehicle_make=ford&amp;vehicle_model=fiesta&amp;distance=50&amp;max_price=2000&amp;min_price=500&amp;photos_filter=true&amp;vehicle_mileage=up_to_80000" />
